@prefix rdfcv: <http://www.dr-thomashartmann.de/phd-thesis/rdf-validation/vocabularies/rdf-constraints-vocabulary#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms#> .
@prefix : <http://example.com/constraints#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#>.
@prefix e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix func: <http://www.w3.org/2007/rif-builtin-function#> .
@prefix pred: <http://www.w3.org/2007/rif-builtin-predicate#> .
@prefix prolog: <http://eulersharp.sourceforge.net/2003/03swap/prolog#>.
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix func: <http://www.w3.org/2007/rif-builtin-function#> .
@base <http://example.com/constraints#> .

{
?constraint a rdfcv:SimpleConstraint ;
  :constraintType :MinimumUnqualifiedCardinalityRestrictions ; 
  rdfcv:constrainingElement :minimum-cardinality ;
  rdfcv:contextClass  ?Class ;
  rdfcv:leftProperties ?p ;
  rdfcv:constrainingValue ?v .

?x a ?Class.
_:x e:findall ( 1 {?x ?p ?o} ?list).
?list e:length ?l.
?l math:lessThan ?v.
}
=>
{
_:x a :constraintViolation;
    :violatedConstraint ?constraint;
    :class ?Class;
    :instance ?x;
    :property ?p.
}.


